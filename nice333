local plr = game.Players.LocalPlayer
local char = plr.Character
local mouse = plr:GetMouse()

local debounce = false
local farming = nil

function StartFarm()
	farming = true
	farm.BackgroundColor3 = Color3.fromRGB(255,0,0)
	wait()
	while farming == true do
		wait(0.6)
		for i,v in pairs(char:GetChildren()) do
			if v.Name == TextBox.Text then
				v:Activate()
			else
				for i,v in pairs(plr.Backpack:GetChildren()) do
					if v.Name == TextBox.Text then
						v.Parent = plr.Character
					end
				end
			end
		end
	end
end

function StopFarm()
	farm.BackgroundColor3 = Color3.fromRGB(0,255,0)
	farming = false
end

farm.MouseButton1Click:Connect(function()
	if debounce == false then 
		debounce = true
		StartFarm()
	else
		debounce = false
		StopFarm()
	end
end)

fps.MouseButton1Down:connect(function()
	loadstring(game:HttpGet("https://pastebin.com/raw/mmEr60d5", true))()
end)

revis.MouseButton1Down:connect(function()
	loadstring(game:HttpGet("https://pastebin.com/raw/pGhYnsxA", true))()
end)

afk.MouseButton1Down:connect(function()
	loadstring(game:HttpGet("https://pastebin.com/raw/SN4agUmB", true))()
end)

hud.MouseButton1Down:connect(function()
	game:GetService("Players").LocalPlayer.PlayerGui.HUD:Destroy()
end)

invortory.MouseButton1Down:connect(function()
	loadstring(game:HttpGet("https://pastebin.com/raw/8W1draqT", true))()
end)

drop.MouseButton1Down:connect(function()
	loadstring(game:HttpGet("https://pastebin.com/raw/CjVD4RZN", true))()
end)

rumble.MouseButton1Click:Connect(function()
	game.ReplicatedFirst.TourneyQ:Destroy()
end)

loop.MouseButton1Click:Connect(function()
	loadstring(game:GetObjects('rbxassetid://1010374652')[1].Source)() 
end)

fastpunch.MouseButton1Click:Connect(function()
	old = hookfunction(wait,function(t)
		if not checkcaller() and getcallingscript().Name == "MouseIcon" then
			return old()
		end
		return old(t)
	end)
end)

small.MouseButton1Click:Connect(function()
	for i,v in pairs(game.Players.LocalPlayer.Character.Humanoid:GetChildren()) do
		if v:IsA("NumberValue") then
			v:Destroy()
		end
	end
end)

afk.MouseButton1Click:Connect(function()
	loadstring(game:HttpGet("https://pastebin.com/raw/SN4agUmB", true))()
end)

smallac.MouseButton1Click:Connect(function()
	game.Players.LocalPlayer.Character.Humanoid.BodyDepthScale.Value = 1
	game.Players.LocalPlayer.Character.Humanoid.BodyHeightScale.Value = 1
	game.Players.LocalPlayer.Character.Humanoid.BodyWidthScale.Value = 1
	game.Players.LocalPlayer.Character.Humanoid.HeadScale.Value = 1
end)

close.MouseButton1Click:Connect(function()
	close.Visible = true
	Frame.Visible = false
	close.Visible = false
end)

popup.MouseButton1Click:Connect(function()
	game:GetService("Players").LocalPlayer.PlayerGui.HUD.Frame.MuscleGain:Destroy()
end)

safe.MouseButton1Click:Connect(function()
	char.HumanoidRootPart.CFrame = CFrame.new(-1941, 353, -5311)
end)

