plr:GetMouse().KeyDown(function(key)
	local key = string.lower(key)
	wait()
	if string.byte(key) == 48 then
		if debounce == true then
			debounce = false
			Main.Visible = false
		else
				debounce = true
				main.Visible = true
		end
	end
end)]]

local debounce = true

mouse.KeyDown:Connect(function(key)
	local key = string.upper(key)
	wait()
	if string.byte(key) == 47 then
		if debounce == true then
			debounce = false
			Frame.Visible = false
		else
			debounce = true
			Frame.Visible = true
		end
	end
end)
